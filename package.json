{
  "name": "occ-poc",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:docker": "docker-compose run --rm app npx prisma migrate deploy",
    "test": "jest -i",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "docker:test": "npm run docker:up && jest -i",
    "docker:test:e2e": "npm run docker:up && jest --config ./test/jest-e2e.json",
    "docker:benchmark": "npm run docker:up && jest --testPathPattern=benchmark-seat-reservation.spec.ts && npm run docker:down",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.1",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.1",
    "@nestjs/platform-express": "^10.4.1",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/swagger": "^7.4.0",
    "@prisma/client": "5.18.0",
    "@types/ioredis": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@zenstackhq/runtime": "^2.4.1",
    "@zenstackhq/server": "^2.4.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.24.7",
    "@jest/globals": "^29.7.0",
    "@nestjs/cli": "^10.4.4",
    "@nestjs/schematics": "^10.1.4",
    "@nestjs/testing": "^10.4.1",
    "@types/express": "^4.17.21",
    "@types/ioredis-mock": "^8.2.5",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.0",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "eslint": "8.57.0",
    "eslint-config-nestjs": "^0.8.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "ioredis": "^5.4.1",
    "jest": "^29.7.0",
    "prisma": "5.18.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4",
    "zenstack": "^2.4.1"
  }
}
